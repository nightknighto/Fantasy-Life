        set udg_CurrentWeapon[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=null
        set udg_Weaponlock[GetConvertedPlayerId(GetOwningPlayer(udg_MortalCombatant))]=0
        call SetUnitLifePercentBJ(udg_MortalCombatant, 100)
        call SetUnitManaPercentBJ(udg_MortalCombatant, 100)
        call UnitAddAbilityBJ('A0BM', udg_MortalCombatant)
