    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=11
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        if ( Trig_Abilities_Additions_Func001Func001C() ) then
            if ( Trig_Abilities_Additions_Func001Func001Func002C() ) then
