                call ModifyHeroStat(bj_HEROSTAT_INT, udg_Hero[GetForLoopIndexA()], bj_MODIFYMETHOD_SET, udg_Intelligence[GetForLoopIndexA()])
                call ModifyHeroStat(bj_HEROSTAT_STR, udg_Hero[GetForLoopIndexA()], bj_MODIFYMETHOD_SET, udg_Strength[GetForLoopIndexA()])
            else
            endif
        else
        endif
