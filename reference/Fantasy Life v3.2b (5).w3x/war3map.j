    call UnitAddAbilityBJ(udg_WeaponAbility1[GetConvertedPlayerId(GetTriggerPlayer())], GetLastReplacedUnitBJ())
    call UnitAddAbilityBJ(udg_WeaponAbility2[GetConvertedPlayerId(GetTriggerPlayer())], GetLastReplacedUnitBJ())
    call UnitAddAbilityBJ(udg_WeaponAbility3[GetConvertedPlayerId(GetTriggerPlayer())], GetLastReplacedUnitBJ())
    set udg_WeaponDamage[GetConvertedPlayerId(GetTriggerPlayer())]=udg_DamageStack[udg_WeaponType[GetConvertedPlayerId(GetTriggerPlayer())]]
    call UnitAddAbilityBJ(udg_WeaponDamage[GetConvertedPlayerId(GetTriggerPlayer())], GetLastReplacedUnitBJ())
    call SetUnitAbilityLevelSwapped(udg_WeaponDamage[GetConvertedPlayerId(GetTriggerPlayer())], GetLastReplacedUnitBJ(), ( GetItemUserData(udg_CurrentWeapon[GetConvertedPlayerId(GetTriggerPlayer())]) - ( ( GetItemUserData(udg_CurrentWeapon[GetConvertedPlayerId(GetTriggerPlayer())]) / 10 ) * 10 ) ))
    call RemoveLocation(udg_Temp_Point)
    set udg_Temp_Point=GetRectCenter(gg_rct_Dummies_Region)
    call SetItemPositionLoc(udg_CurrentWeapon[GetConvertedPlayerId(GetTriggerPlayer())], udg_Temp_Point)
    call RemoveLocation(udg_Temp_Point)
