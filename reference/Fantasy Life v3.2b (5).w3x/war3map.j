        call ForForce(GetPlayersAll(), function Trig_Cultist_Defeat_Func012Func019A)
        set udg_Temp_Group=GetUnitsInRectOfPlayer(gg_rct_Cultist_Lair, Player(11))
        call ForGroupBJ(udg_Temp_Group, function Trig_Cultist_Defeat_Func012Func028A)
        call DestroyGroup(udg_Temp_Group)
        call PauseAllUnitsBJ(false)
        call CinematicModeBJ(false, GetPlayersAll())
        call SetUserControlForceOn(GetPlayersAll())
        set udg_Cinematic=false
    else
        call PauseAllUnitsBJ(true)
        set udg_UNITGROUP[1]=GetUnitsInRectMatching(gg_rct_Cultist_Lair, Condition(function Trig_Cultist_Defeat_Func012Func003002002))
        call ForGroupBJ(udg_UNITGROUP[1], function Trig_Cultist_Defeat_Func012Func004A)
        call DestroyGroup(udg_UNITGROUP[1])
        call CinematicModeBJ(true, udg_CinematicViewers)
        call SetUserControlForceOff(udg_CinematicViewers)
        set udg_Cinematic=true
        call ForForce(udg_CinematicViewers, function Trig_Cultist_Defeat_Func012Func009A)
        call TransmissionFromUnitWithNameBJ(udg_CinematicViewers, gg_unit_H047_0163, "TRIGSTR_3165", null, "TRIGSTR_3166", bj_TIMETYPE_SUB, 0.00, true)
        call ForForce(udg_CinematicViewers, function Trig_Cultist_Defeat_Func012Func011A)
        call TransmissionFromUnitWithNameBJ(udg_CinematicViewers, gg_unit_H047_0163, "TRIGSTR_3167", null, "TRIGSTR_3168", bj_TIMETYPE_ADD, 2.00, true)
        set udg_Temp_Group=GetUnitsInRectOfPlayer(gg_rct_Cultist_Lair, Player(11))
        call ForGroupBJ(GetUnitsInRectOfPlayer(gg_rct_Cultist_Lair, Player(11)), function Trig_Cultist_Defeat_Func012Func021A)
        call DestroyGroup(udg_Temp_Group)
        call PauseAllUnitsBJ(false)
        set udg_Cinematic=false
        call CinematicModeBJ(false, GetPlayersAll())
        call SetUserControlForceOn(GetPlayersAll())
        call DestroyForce(udg_CinematicViewers)
    endif
    call TriggerSleepAction(60.00)
    set udg_EventRunning=false
    call ForForce(GetPlayersAll(), function Trig_Cultist_Defeat_Func015A)
    call TriggerSleepAction(480.00)
    call ConditionalTriggerExecute(gg_trg_Randomizer)
