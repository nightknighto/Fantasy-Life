function Trig_Deposit_Func002C takes nothing returns boolean
    if ( not ( S2I(SubStringBJ(GetEventPlayerChatString(), 10, StringLength(GetEventPlayerChatString()))) <= GetPlayerState(GetTriggerPlayer(), PLAYER_STATE_RESOURCE_GOLD) ) ) then
        return false
    endif
    return true
