        return false
    endif
    return true
endfunction

function Trig_Player_Exits_Coliseum_Func002Func002C takes nothing returns boolean
    if ( not ( UnitInventoryCount(udg_Gladiator) == 6 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Player_Exits_Coliseum_Func002Func003001 takes nothing returns boolean
    return ( udg_TriforceofCourage == false )
endfunction

function Trig_Player_Exits_Coliseum_Func002Func003002 takes nothing returns boolean
    return ( udg_Round == 11 )
endfunction

function Trig_Player_Exits_Coliseum_Func002C takes nothing returns boolean
    if ( not GetBooleanAnd((udg_TriforceofCourage == false), (udg_Round == 11)) ) then // INLINED!!
        return false
    endif
    return true
endfunction

function Trig_Player_Exits_Coliseum_Actions takes nothing returns nothing
    set udg_InDialouge[GetConvertedPlayerId(GetTriggerPlayer())]=false
    if ( Trig_Player_Exits_Coliseum_Func002C() ) then
        set udg_TriforceofCourage=true
        if ( Trig_Player_Exits_Coliseum_Func002Func002C() ) then
            set udg_POINT[103]=GetRectCenter(gg_rct_Coliseum_Entrance)
            call CreateItemLoc('I00K', udg_POINT[103])
            call RemoveLocation(udg_POINT[103])
            set udg_Temp_Player_Group=GetForceOfPlayer(udg_GladiatorPlayer)
            call DisplayTextToForce(udg_Temp_Player_Group, "TRIGSTR_3558")
            call DestroyForce(udg_Temp_Player_Group)
        else
            call UnitAddItemByIdSwapped('I00K', udg_Gladiator)
            set udg_Temp_Player_Group=GetForceOfPlayer(udg_GladiatorPlayer)
            call DisplayTextToForce(udg_Temp_Player_Group, "TRIGSTR_820")
            call DestroyForce(udg_Temp_Player_Group)
        endif
    else
    endif
    call PauseTimerBJ(true, udg_ColiseumTime)
    call DestroyTimerDialogBJ(udg_ColiseumTimer)
    set udg_POINT[103]=GetRectCenter(gg_rct_Coliseum_Entrance)
    call SetUnitPositionLoc(udg_Gladiator, udg_POINT[103])
    call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_Gladiator), udg_POINT[103], 0)
    call RemoveLocation(udg_POINT[103])
    call CameraSetSmoothingFactorBJ(1.00)
    call ResetToGameCameraForPlayer(udg_GladiatorPlayer, 0)
    set udg_Gladiator=null
    call RemoveUnit(udg_Opponent)
    set udg_Opponent=null
    call AdjustPlayerStateBJ(( ( udg_Round - 1 ) * 100 ), udg_GladiatorPlayer, PLAYER_STATE_RESOURCE_GOLD)
    set udg_Temp_Player_Group=GetForceOfPlayer(udg_GladiatorPlayer)
    call DisplayTextToForce(udg_Temp_Player_Group, ( "Won " + ( I2S(( ( udg_Round - 1 ) * 100 )) + " gold!" ) ))
    call DestroyForce(udg_Temp_Player_Group)
    call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_133")
    call TriggerSleepAction(30.00)
    call EnableTrigger(gg_trg_Player_Starts_Coliseum)
    call EnableTrigger(gg_trg_Player_Enters_Coliseum)
    call EnableTrigger(gg_trg_PVP_circle_1)
    call EnableTrigger(gg_trg_PVP_circle_2)
endfunction

