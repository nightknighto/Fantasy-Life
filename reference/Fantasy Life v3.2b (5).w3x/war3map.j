    call SelectUnitForPlayerSingle(GetTriggerUnit(), GetOwningPlayer(GetTriggerUnit()))
    call ModifyHeroStat(bj_HEROSTAT_STR, GetTriggerUnit(), bj_MODIFYMETHOD_SET, R2I(( 2.00 * I2R(udg_Strength[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]) )))
    call ModifyHeroStat(bj_HEROSTAT_INT, GetTriggerUnit(), bj_MODIFYMETHOD_SET, udg_Intelligence[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))])
    call ModifyHeroStat(bj_HEROSTAT_AGI, GetTriggerUnit(), bj_MODIFYMETHOD_SET, udg_Agility[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))])
    call UnitAddAbilityBJ(udg_UHP[GetHeroLevel(GetTriggerUnit())], GetTriggerUnit())
    call UnitAddAbilityBJ(udg_BaseClass[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))], GetTriggerUnit())
    if ( Trig_Nosferatu_Drinks_Func023C() ) then
        call SetUnitAbilityLevelSwapped(udg_BaseClass[GetConvertedPlayerId(GetTriggerPlayer())], GetTriggerUnit(), 5)
