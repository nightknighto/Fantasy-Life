    if ( not Trig_Donations_Func003Func015C() ) then
        return false
    endif
    return true
endfunction

function Trig_Donations_Actions takes nothing returns nothing
    if ( Trig_Donations_Func001C() ) then
        set udg_Happiness=( udg_Happiness - 150 )
    else
    endif
    set udg_HappinessContribution[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=( udg_HappinessContribution[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] + 100 )
    if ( Trig_Donations_Func003C() ) then
        set udg_Temp_Player_Group=GetForceOfPlayer(GetOwningPlayer(GetManipulatingUnit()))
        call DisplayTextToForce(udg_Temp_Player_Group, "TRIGSTR_412")
        call DestroyForce(udg_Temp_Player_Group)
        call AdjustPlayerStateBJ(500, GetOwningPlayer(GetTriggerUnit()), PLAYER_STATE_RESOURCE_GOLD)
        set udg_Happiness=( udg_Happiness + 150 )
        set udg_HappinessContribution[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=( udg_HappinessContribution[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] - 100 )
    else
        call SetUnitUserData(GetTriggerUnit(), 0)
        set udg_WolfPlayers[GetConvertedPlayerId(GetOwningPlayer(GetManipulatingUnit()))]=0
        set udg_Temp_Player_Group=GetForceOfPlayer(GetOwningPlayer(GetManipulatingUnit()))
        call DisplayTextToForce(udg_Temp_Player_Group, "TRIGSTR_1862")
        call DestroyForce(udg_Temp_Player_Group)
        set udg_DonationNUMBER[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=( udg_DonationNUMBER[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] + 1 )
        if ( Trig_Donations_Func003Func013C() ) then
            call UnitAddItemByIdSwapped('I00M', udg_Hero[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))])
            set udg_DonationNUMBER[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=- 10
        else
        endif
        if ( Trig_Donations_Func003Func014C() ) then
            set udg_Fame[12]=( udg_Fame[12] - 3 )
            set udg_Fame[GetConvertedPlayerId(GetOwningPlayer(GetManipulatingUnit()))]=( udg_Fame[GetConvertedPlayerId(GetOwningPlayer(GetManipulatingUnit()))] + 3 )
        else
        endif
    endif
endfunction

