function Trig_Equip_Actions takes nothing returns nothing
    if ( Trig_Equip_Func003C() ) then
        set udg_Boolean[91]=true
    else
    endif
    if ( Trig_Equip_Func004C() ) then
        set udg_Boolean[92]=true
    else
    endif
    if ( Trig_Equip_Func005C() ) then
        set udg_Boolean[93]=true
    else
    endif
    call SelectUnitRemove(GetTriggerUnit())
    set udg_Temp_Point=GetUnitLoc(GetTriggerUnit())
    call SetItemPositionLoc(udg_CurrentWeapon[GetConvertedPlayerId(GetTriggerPlayer())], udg_Temp_Point)
    call UnitAddItemSwapped(udg_CurrentWeapon[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))], GetTriggerUnit())
    set udg_CurrentWeapon[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=GetManipulatedItem()
    set udg_WeaponType[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=( GetItemUserData(GetManipulatedItem()) / 100000 )
    set udg_WeaponAbility1[GetConvertedPlayerId(GetTriggerPlayer())]=udg_AbilityStack[LoadIntegerBJ(udg_WeaponType[GetConvertedPlayerId(GetTriggerPlayer())], ( ( GetItemUserData(GetManipulatedItem()) / 10000 ) - ( udg_WeaponType[GetConvertedPlayerId(GetTriggerPlayer())] * 10 ) ), udg_WeaponSkillsTable)]
    set udg_WeaponAbility2[GetConvertedPlayerId(GetTriggerPlayer())]=udg_AbilityStack[LoadIntegerBJ(( ( GetItemUserData(GetManipulatedItem()) / 1000 ) - ( ( GetItemUserData(GetManipulatedItem()) / 10000 ) * 10 ) ), ( udg_WeaponType[GetConvertedPlayerId(GetTriggerPlayer())] * 10 ), udg_WeaponSkillsTable)]
    set udg_WeaponAbility3[GetConvertedPlayerId(GetTriggerPlayer())]=udg_AbilityStack[LoadIntegerBJ(( ( GetItemUserData(GetManipulatedItem()) / 10 ) - ( ( GetItemUserData(GetManipulatedItem()) / 1000 ) * 10 ) ), ( ( GetItemUserData(GetManipulatedItem()) / 100 ) - ( ( GetItemUserData(GetManipulatedItem()) / 10000 ) * 100 ) ), udg_WeaponSkillsTable)]
    set udg_Temp_HP=GetUnitStateSwap(UNIT_STATE_LIFE, GetTriggerUnit())
    set udg_Temp_MP=GetUnitStateSwap(UNIT_STATE_MANA, GetTriggerUnit())
    if ( Trig_Equip_Func017C() ) then
        if ( Trig_Equip_Func017Func002C() ) then
            call ReplaceUnitBJ(GetTriggerUnit(), 'H053', bj_UNIT_STATE_METHOD_ABSOLUTE)
        else
            if ( Trig_Equip_Func017Func002Func001C() ) then
                call ReplaceUnitBJ(GetTriggerUnit(), 'H065', bj_UNIT_STATE_METHOD_ABSOLUTE)
            else
                call ReplaceUnitBJ(GetTriggerUnit(), udg_Model[LoadIntegerBJ(udg_WeaponType[GetConvertedPlayerId(GetTriggerPlayer())], udg_RaceNGender[GetConvertedPlayerId(GetTriggerPlayer())], udg_EquipmentTable)], bj_UNIT_STATE_METHOD_ABSOLUTE)
            endif
        endif
    else
        call ReplaceUnitBJ(GetTriggerUnit(), udg_Model[LoadIntegerBJ(udg_WeaponType[GetConvertedPlayerId(GetTriggerPlayer())], udg_RaceNGender[GetConvertedPlayerId(GetTriggerPlayer())], udg_EquipmentTable)], bj_UNIT_STATE_METHOD_ABSOLUTE)
    endif
    call SelectUnitForPlayerSingle(GetLastReplacedUnitBJ(), GetOwningPlayer(GetLastReplacedUnitBJ()))
    set udg_Hero[GetConvertedPlayerId(GetTriggerPlayer())]=GetLastReplacedUnitBJ()
    if ( Trig_Equip_Func020C() ) then
