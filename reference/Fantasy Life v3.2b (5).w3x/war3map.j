function Trig_Fire_Breath_Func002002003 takes nothing returns boolean
    return ( IsPlayerEnemy(GetOwningPlayer(GetFilterUnit()), GetOwningPlayer(GetTriggerUnit())) == true )
endfunction

function Trig_Fire_Breath_Func004Func004C takes nothing returns boolean
    if ( not ( AngleBetweenPoints(udg_Caster, udg_Picked) <= ( AngleBetweenPoints(udg_Caster, udg_Temp_Point) + 45.00 ) ) ) then
        return false
    endif
    if ( not ( AngleBetweenPoints(udg_Caster, udg_Picked) >= ( AngleBetweenPoints(udg_Caster, udg_Temp_Point) - 45.00 ) ) ) then
        return false
    endif
    return true
endfunction

