    if ( Trig_Equip_Func034C() ) then
        call SetUnitMoveSpeed(GetLastReplacedUnitBJ(), ( GetUnitDefaultMoveSpeed(GetLastReplacedUnitBJ()) + ( ( I2R(( GetItemUserData(udg_CurrentWeapon[GetConvertedPlayerId(GetTriggerPlayer())]) / 10000 )) - 20.00 ) * 20.00 ) ))
