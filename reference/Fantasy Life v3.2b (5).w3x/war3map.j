function Trig_Bandit_Spawns_Actions takes nothing returns nothing
    if ( Trig_Bandit_Spawns_Func001C() ) then
        set udg_Happiness=2400
    else
    endif
    if ( Trig_Bandit_Spawns_Func002C() ) then
        set udg_Happiness=300
    else
    endif
    set udg_BanditTemp=( 5.00 * ( ( ( I2R(udg_Happiness) / 1000.00 ) * ( I2R(udg_Happiness) / 1000.00 ) ) - ( ( I2R(udg_Happiness) / 1000.00 ) - 2.00 ) ) )
    call DestroyGroup(udg_BanditGroup)
    set udg_BanditGroup=GetUnitsInRectMatching(gg_rct_Western_Highway, Condition(function Trig_Bandit_Spawns_Func005002002))
    if ( Trig_Bandit_Spawns_Func006C() ) then
        set bj_forLoopAIndex=1
        set bj_forLoopAIndexEnd=R2I(( ( udg_BanditTemp * 1.80 ) - I2R(CountUnitsInGroup(udg_BanditGroup)) ))
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            set udg_Temp_Point_2=GetRandomLocInRect(gg_rct_Western_Highway)
            call CreateNUnitsAtLoc(1, 'h02L', Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_Temp_Point_2, bj_UNIT_FACING)
            call RemoveLocation(udg_Temp_Point_2)
            set udg_Temp_Point_2=GetRandomLocInRect(gg_rct_Western_Highway)
            call CreateNUnitsAtLoc(1, 'h038', Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_Temp_Point_2, bj_UNIT_FACING)
            call RemoveLocation(udg_Temp_Point_2)
            set bj_forLoopAIndex=bj_forLoopAIndex + 1
        endloop
    else
    endif
    call DestroyGroup(udg_BanditGroup)
    set udg_BanditGroup=GetUnitsInRectMatching(gg_rct_Slums, Condition(function Trig_Bandit_Spawns_Func008002002))
    if ( Trig_Bandit_Spawns_Func009C() ) then
        set bj_forLoopAIndex=1
        set bj_forLoopAIndexEnd=R2I(( ( udg_BanditTemp * 0.20 ) - I2R(CountUnitsInGroup(udg_BanditGroup)) ))
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            set udg_Temp_Point_2=GetRandomLocInRect(gg_rct_Slums)
            call CreateNUnitsAtLoc(1, 'h02L', Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_Temp_Point_2, bj_UNIT_FACING)
            call RemoveLocation(udg_Temp_Point_2)
            set udg_Temp_Point_2=GetRandomLocInRect(gg_rct_Slums)
            call CreateNUnitsAtLoc(1, 'h038', Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_Temp_Point_2, bj_UNIT_FACING)
            call RemoveLocation(udg_Temp_Point_2)
            set bj_forLoopAIndex=bj_forLoopAIndex + 1
