    set udg_Caster=GetUnitLoc(GetSpellAbilityUnit())
    set udg_Picked=GetUnitLoc(GetEnumUnit())
    set udg_Temp_Point=GetSpellTargetLoc()
    if ( Trig_Fire_Breath_Func004Func004C() ) then
        call UnitDamageTargetBJ(GetSpellAbilityUnit(), GetEnumUnit(), ( I2R(GetHeroStatBJ(bj_HEROSTAT_AGI, GetSpellAbilityUnit(), true)) * 4.00 ), ATTACK_TYPE_NORMAL, DAMAGE_TYPE_NORMAL)
    else
    endif
    call RemoveLocation(udg_Temp_Point)
    call RemoveLocation(udg_Caster)
    call RemoveLocation(udg_Picked)
endfunction

