    call CinematicModeBJ(false, GetPlayersAll())
    call SetUserControlForceOn(GetPlayersAll())
    set udg_NumberOfEvents=1
    set udg_Events[1]=gg_trg_The_Lord
    call SaveStringBJ("Hey there! Sit down and grab a pint, I'm about to sing one of my favorite songs!", StringHashBJ(UnitId2StringBJ('n00N')), 0, udg_TalkingTable)
    set udg_EventRunning=false
    call EnableTrigger(gg_trg_Unknown_Dialogue)
    call TriggerSleepAction(60.00)
    call RemoveUnit(udg_BossUnit)
    call ForForce(GetPlayersAll(), function Trig_Demon_General_Victory_Func043A)
    call TriggerSleepAction(480.00)
    call ConditionalTriggerExecute(gg_trg_Randomizer)
    call ForGroupBJ(udg_UNITGROUP[200], function Trig_Demon_General_Victory_Func046A)
