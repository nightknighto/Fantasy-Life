        return false
    endif
    return true
endfunction

function Trig_Human_Form_Func017C takes nothing returns boolean
    if ( not ( udg_Boolean[92] == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_Human_Form_Func018C takes nothing returns boolean
    if ( not ( udg_Boolean[93] == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_Human_Form_Actions takes nothing returns nothing
    // Werewolf
    if ( Trig_Human_Form_Func002C() ) then
        set udg_Boolean[91]=true
    else
    endif
    if ( Trig_Human_Form_Func003C() ) then
        set udg_Boolean[92]=true
    else
    endif
    if ( Trig_Human_Form_Func004C() ) then
        set udg_Boolean[93]=true
    else
    endif
    if ( Trig_Human_Form_Func005C() ) then
        call ShowUnitShow(udg_Hero[GetUnitUserData(GetTriggerUnit())])
        call SetUnitPositionLoc(udg_Hero[GetUnitUserData(GetTriggerUnit())], GetUnitLoc(GetTriggerUnit()))
        set bj_forLoopAIndex=1
        set bj_forLoopAIndexEnd=6
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            call UnitAddItemSwapped(UnitItemInSlotBJ(GetTriggerUnit(), GetForLoopIndexA()), udg_Hero[GetUnitUserData(GetTriggerUnit())])
            set bj_forLoopAIndex=bj_forLoopAIndex + 1
        endloop
        call UnitAddAbilityBJ('A0BM', udg_Hero[GetUnitUserData(GetTriggerUnit())])
        call IssueImmediateOrderBJ(udg_Hero[GetUnitUserData(GetTriggerUnit())], "divineshield")
        call RemoveUnit(GetTriggerUnit())
        return
    else
    endif
    call SelectUnitRemove(GetTriggerUnit())
    set udg_Temp_Point=GetUnitLoc(GetTriggerUnit())
    call SetItemPositionLoc(udg_CurrentWeapon[GetConvertedPlayerId(GetTriggerPlayer())], udg_Temp_Point)
    call UnitAddItemSwapped(udg_CurrentWeapon[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))], GetTriggerUnit())
    set udg_CurrentWeapon[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=null
    set udg_WeaponType[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=0
    set udg_Temp_HP=GetUnitStateSwap(UNIT_STATE_LIFE, GetTriggerUnit())
    set udg_Temp_MP=GetUnitStateSwap(UNIT_STATE_MANA, GetTriggerUnit())
    if ( Trig_Human_Form_Func014C() ) then
        call ReplaceUnitBJ(GetTriggerUnit(), 'H06V', bj_UNIT_STATE_METHOD_RELATIVE)
    else
        call ReplaceUnitBJ(GetTriggerUnit(), udg_Model[LoadIntegerBJ(udg_WeaponType[GetConvertedPlayerId(GetTriggerPlayer())], udg_RaceNGender[GetConvertedPlayerId(GetTriggerPlayer())], udg_EquipmentTable)], bj_UNIT_STATE_METHOD_RELATIVE)
    endif
    call SelectUnitForPlayerSingle(GetLastReplacedUnitBJ(), GetOwningPlayer(GetLastReplacedUnitBJ()))
    if ( Trig_Human_Form_Func016C() ) then
        set udg_Gladiator=GetLastReplacedUnitBJ()
        set udg_Boolean[91]=false
    else
    endif
    if ( Trig_Human_Form_Func017C() ) then
        set udg_PVPUNIT1=GetLastReplacedUnitBJ()
        set udg_Boolean[92]=false
    else
    endif
    if ( Trig_Human_Form_Func018C() ) then
        set udg_PVPUNIT2=GetLastReplacedUnitBJ()
        set udg_Boolean[93]=false
    else
    endif
    set udg_Hero[GetConvertedPlayerId(GetTriggerPlayer())]=GetLastReplacedUnitBJ()
    call UnitAddAbilityBJ(udg_JobAbility[GetUnitUserData(udg_Job[GetConvertedPlayerId(GetTriggerPlayer())])], GetLastReplacedUnitBJ())
    call TriggerExecute(gg_trg_Skills)
endfunction

