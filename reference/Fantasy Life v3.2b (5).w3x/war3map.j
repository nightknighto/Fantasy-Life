    else
        call RemoveUnit(GetTriggerUnit())
        set udg_FriendshipRing[GetConvertedPlayerId(GetOwningPlayer(udg_MortalCombatant))]=GetItemOfTypeFromUnitBJ(udg_MortalCombatant, 'I019')
        call UnitRemoveItemFromSlotSwapped(1, udg_MortalCombatant)
