    endif
    call UnitAddAbilityBJ(udg_MasterClass[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))], GetTriggerUnit())
    if ( Trig_Nosferatu_Drinks_Func025C() ) then
        call SetUnitAbilityLevelSwapped(udg_MasterClass[GetConvertedPlayerId(GetTriggerPlayer())], GetTriggerUnit(), 8)
    else
