    if ( not ( UnitHasBuffBJ(GetTriggerUnit(), 'B013') == true ) ) then
        return false
    endif
    if ( not ( UnitHasBuffBJ(GetTriggerUnit(), 'B00F') == false ) ) then
        return false
    endif
    if ( not ( GetUnitLifePercent(GetTriggerUnit()) <= 20.00 ) ) then
        return false
    endif
    return true
endfunction

