    call TriggerAddAction(gg_trg_Lancet, function Trig_Lancet_Actions)
endfunction

//===========================================================================
// Trigger: Dragon Soul
//===========================================================================
function Trig_Dragon_Soul_Func010C takes nothing returns boolean
    if ( ( GetUnitAbilityLevelSwapped('S000', GetTriggerUnit()) != 0 ) ) then
        return true
    endif
    if ( ( ( udg_Gametime - udg_DragonSoul[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] ) >= 120 ) ) then
        return true
    endif
    return false
endfunction

function Trig_Dragon_Soul_Conditions takes nothing returns boolean
    if ( not ( ( GetUnitStateSwap(UNIT_STATE_LIFE, GetTriggerUnit()) - GetEventDamage() ) <= 0.00 ) ) then
        return false
    endif
    if ( not ( GetUnitAbilityLevelSwapped('A07B', GetTriggerUnit()) != 0 ) ) then
        return false
    endif
    if ( not Trig_Dragon_Soul_Func010C() ) then
        return false
    endif
    return true
endfunction

function Trig_Dragon_Soul_Func006A takes nothing returns nothing
    call SetItemVisibleBJ(false, GetEnumItem())
endfunction

function Trig_Dragon_Soul_Func019A takes nothing returns nothing
    call SetItemVisibleBJ(true, GetEnumItem())
    call UnitAddItemSwapped(GetEnumItem(), udg_Hero[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))])
endfunction

function Trig_Dragon_Soul_Func020C takes nothing returns boolean
    if ( not ( GetOwningPlayer(GetTriggerUnit()) == Player(11) ) ) then
        return false
    endif
    return true
endfunction

function Trig_Dragon_Soul_Actions takes nothing returns nothing
    set udg_POINT[103]=GetUnitLoc(GetTriggerUnit())
    call CreateFogModifierRectBJ(true, GetTriggerPlayer(), FOG_OF_WAR_VISIBLE, RectFromCenterSizeBJ(udg_POINT[103], 500.00, 500.00))
    call RemoveLocation(udg_POINT[103])
    set udg_SoulVision[GetConvertedPlayerId(GetTriggerPlayer())]=GetLastCreatedFogModifier()
    set udg_Temp_Point=GetUnitLoc(GetTriggerUnit())
    call EnumItemsInRectBJ(RectFromCenterSizeBJ(udg_Temp_Point, 200.00, 200.00), function Trig_Dragon_Soul_Func006A)
    call RemoveLocation(udg_Temp_Point)
    call TriggerSleepAction(3.00)
    set udg_Temp_Point=GetUnitLoc(GetTriggerUnit())
    call CreateNUnitsAtLoc(1, 'u001', GetOwningPlayer(GetTriggerUnit()), udg_Temp_Point, bj_UNIT_FACING)
    call SetUnitScalePercent(GetLastCreatedUnit(), 75.00, 75.00, 75.00)
    call UnitApplyTimedLifeBJ(2.00, 'Bapl', GetLastCreatedUnit())
    call ReviveHeroLoc(udg_Hero[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))], udg_Temp_Point, false)
    call RemoveLocation(udg_Temp_Point)
    set udg_Temp_Point=GetUnitLoc(GetTriggerUnit())
    call EnumItemsInRectBJ(RectFromCenterSizeBJ(udg_Temp_Point, 200.00, 200.00), function Trig_Dragon_Soul_Func019A)
    if ( Trig_Dragon_Soul_Func020C() ) then
        call UnitRemoveAbilityBJ('S000', GetTriggerUnit())
    else
    endif
    call DestroyFogModifier(udg_SoulVision[GetConvertedPlayerId(GetTriggerPlayer())])
    set udg_DragonSoul[GetConvertedPlayerId(GetTriggerPlayer())]=udg_Gametime
    call SetUnitLifeBJ(GetTriggerUnit(), 100.00)
    call SetUnitManaBJ(GetRevivingUnit(), 100.00)
    call RemoveLocation(udg_Temp_Point)
endfunction

