    call DisableTrigger(gg_trg_Army_to_Church)
    call PauseUnitBJ(true, udg_BossUnit)
    call DisableTrigger(gg_trg_Unknown_Dialogue)
    call CinematicModeBJ(true, GetPlayersAll())
    call SetUserControlForceOff(GetPlayersAll())
    set udg_Cinematic=true
    call PauseAllUnitsBJ(true)
    call ForForce(GetPlayersAll(), function Trig_Demon_General_Victory_Func018A)
    call TransmissionFromUnitWithNameBJ(GetPlayersAll(), udg_BossUnit, "TRIGSTR_2703", null, "TRIGSTR_2706", bj_TIMETYPE_ADD, 2.00, true)
    call TransmissionFromUnitWithNameBJ(GetPlayersAll(), udg_BossUnit, "TRIGSTR_2708", null, "TRIGSTR_2709", bj_TIMETYPE_ADD, 2.00, true)
    call TransmissionFromUnitWithNameBJ(GetPlayersAll(), udg_BossUnit, "TRIGSTR_2712", null, "TRIGSTR_2713", bj_TIMETYPE_SUB, 2.00, true)
    call KillUnit(udg_BossUnit)
