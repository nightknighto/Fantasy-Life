function Trig_Mod_Added_Conditions takes nothing returns boolean
    if ( not Trig_Mod_Added_Func002C() ) then
        return false
    endif
    return true
endfunction

function Trig_Mod_Added_Func001C takes nothing returns boolean
    if ( not ( CheckItemcodeStatus(GetItemTypeId(udg_CurrentWeapon[GetConvertedPlayerId(GetTriggerPlayer())]), bj_ITEMCODE_STATUS_SELLABLE) == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_Mod_Added_Actions takes nothing returns nothing
    if ( Trig_Mod_Added_Func001C() ) then
        set udg_Temp_int=( ( GetItemUserData(udg_CurrentWeapon[GetConvertedPlayerId(GetTriggerPlayer())]) / 1000 ) * 10 )
        set udg_Temp_int=( ( udg_Temp_int + LoadIntegerBJ(0, StringHashBJ(SubStringBJ(GetItemName(GetManipulatedItem()), 11, ( StringLength(GetItemName(GetManipulatedItem())) - 2 ))), udg_EquipmentTable) ) * 10 )
        set udg_Temp_int=( ( GetItemUserData(udg_CurrentWeapon[GetConvertedPlayerId(GetTriggerPlayer())]) - ( ( GetItemUserData(udg_CurrentWeapon[GetConvertedPlayerId(GetTriggerPlayer())]) / 100 ) * 100 ) ) + udg_Temp_int )
        call SetItemUserData(udg_CurrentWeapon[GetConvertedPlayerId(GetTriggerPlayer())], udg_Temp_int)
        set udg_WeaponAbility3[GetConvertedPlayerId(GetTriggerPlayer())]=udg_AbilityStack[LoadIntegerBJ(( ( GetItemUserData(udg_CurrentWeapon[GetConvertedPlayerId(GetTriggerPlayer())]) / 10 ) - ( ( GetItemUserData(udg_CurrentWeapon[GetConvertedPlayerId(GetTriggerPlayer())]) / 10000 ) * 10 ) ), ( ( GetItemUserData(udg_CurrentWeapon[GetConvertedPlayerId(GetTriggerPlayer())]) / 100 ) - ( ( GetItemUserData(udg_CurrentWeapon[GetConvertedPlayerId(GetTriggerPlayer())]) / 10000 ) * 100 ) ), udg_WeaponSkillsTable)]
        call UnitAddAbilityBJ(udg_WeaponAbility3[GetConvertedPlayerId(GetTriggerPlayer())], GetTriggerUnit())
    else
        call UnitAddItemByIdSwapped(GetItemTypeId(GetManipulatedItem()), GetTriggerUnit())
    endif
endfunction

