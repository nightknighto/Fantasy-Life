    return true
endfunction

function Trig_Show_Fame_Func002Func001C takes nothing returns boolean
    if ( not ( udg_Fame[GetForLoopIndexA()] != 0 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Show_Fame_Func003Func001C takes nothing returns boolean
    if ( not ( udg_Fame_Temp[GetForLoopIndexA()] != 0 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Show_Fame_Actions takes nothing returns nothing
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=12
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        if ( Trig_Show_Fame_Func002Func001C() ) then
            call DisplayTextToForce(GetPlayersAll(), ( ( "Player " + I2S(GetForLoopIndexA()) ) + ( " has " + ( I2S(udg_Fame[GetForLoopIndexA()]) + " Fame" ) ) ))
        else
        endif
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=17
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        if ( Trig_Show_Fame_Func003Func001C() ) then
            call DisplayTextToForce(GetPlayersAll(), ( I2S(GetForLoopIndexA()) + ( " has " + ( I2S(udg_Fame_Temp[GetForLoopIndexA()]) + " Temp_Fame" ) ) ))
