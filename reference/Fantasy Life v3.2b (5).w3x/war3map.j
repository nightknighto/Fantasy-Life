function Trig_Treasury_is_Robbed_Conditions takes nothing returns boolean
    if ( not ( IsUnitType(GetEnteringUnit(), UNIT_TYPE_HERO) == true ) ) then
        return false
    endif
    if ( not ( GetPlayerController(GetOwningPlayer(GetEnteringUnit())) == MAP_CONTROL_USER ) ) then
        return false
    endif
    if ( not ( GetPlayerController(GetOwningPlayer(gg_unit_h050_0042)) == MAP_CONTROL_USER ) ) then
        return false
    endif
    if ( not ( GetOwningPlayer(GetEnteringUnit()) != Player(11) ) ) then
        return false
    endif
    if ( not ( GetOwningPlayer(gg_unit_h050_0042) != GetOwningPlayer(GetEnteringUnit()) ) ) then
        return false
    endif
    if ( not ( ( udg_Robbed[2] + 60 ) < udg_Gametime ) ) then
        return false
    endif
    return true
endfunction

function Trig_Treasury_is_Robbed_Func011002001 takes nothing returns boolean
    return ( GetOwningPlayer(GetEnteringUnit()) == GetFilterPlayer() )
endfunction

function Trig_Treasury_is_Robbed_Actions takes nothing returns nothing
    call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_4463")
    set udg_Region=GetRectCenter(gg_rct_Steal_from_Treasury)
    call PingMinimapLocForForceEx(GetPlayersAll(), udg_Region, 5.00, bj_MINIMAPPINGSTYLE_ATTACK, 100, 100, 100)
    call RemoveLocation(udg_Region)
    set udg_Player_Group=GetPlayersMatching(Condition(function Trig_Treasury_is_Robbed_Func011002001))
    call DisplayTextToForce(udg_Player_Group, ( "Stole " + ( I2S(( GetPlayerState(GetOwningPlayer(gg_unit_h050_0042), PLAYER_STATE_RESOURCE_GOLD) / 4 )) + " gold!" ) ))
    call DestroyForce(udg_Player_Group)
    call AdjustPlayerStateBJ(( GetPlayerState(GetOwningPlayer(gg_unit_h050_0042), PLAYER_STATE_RESOURCE_GOLD) / 4 ), GetOwningPlayer(GetEnteringUnit()), PLAYER_STATE_RESOURCE_GOLD)
    call SetPlayerStateBJ(GetOwningPlayer(gg_unit_h050_0042), PLAYER_STATE_RESOURCE_GOLD, ( GetPlayerState(GetOwningPlayer(gg_unit_h050_0042), PLAYER_STATE_RESOURCE_GOLD) - ( GetPlayerState(GetOwningPlayer(gg_unit_h050_0042), PLAYER_STATE_RESOURCE_GOLD) / 4 ) ))
    set udg_Robbed[2]=udg_Gametime
endfunction

