function Trig_Banshee_Attacked_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    set udg_Temp_Group2=GetUnitsInRectAll(gg_rct_Hall_of_Heroes)
    call SetUnitInvulnerable(udg_BossUnit, true)
    call ForGroupBJ(udg_Temp_Group2, function Trig_Banshee_Attacked_Func004A)
    set udg_POINT[100]=GetUnitLoc(GetTriggerUnit())
    call CreateNUnitsAtLoc(1, 'u00P', Player(11), udg_POINT[100], bj_UNIT_FACING)
    call UnitApplyTimedLifeBJ(1.00, 'Bapl', GetLastCreatedUnit())
    call RemoveLocation(udg_POINT[100])
    call TriggerSleepAction(1.00)
    set udg_TimePossessed=udg_Gametime
    set udg_POINT[100]=GetRectCenter(gg_rct_Dummies_Region)
    call SetUnitPositionLoc(GetTriggerUnit(), udg_POINT[100])
    call RemoveLocation(udg_POINT[100])
    call PauseUnitBJ(true, GetTriggerUnit())
    call TriggerSleepAction(0.20)
    set udg_Statue=GroupPickRandomUnit(GetUnitsInRectMatching(gg_rct_Hall_of_Heroes, Condition(function Trig_Banshee_Attacked_Func016002001002)))
    call SetUnitOwner(udg_Statue, Player(11), false)
    set udg_POINT[100]=GetUnitLoc(udg_Statue)
    call CreateNUnitsAtLoc(1, 'u00P', Player(11), udg_POINT[100], bj_UNIT_FACING)
    call UnitApplyTimedLifeBJ(1.00, 'Bapl', GetLastCreatedUnit())
    call RemoveLocation(udg_POINT[100])
    call SetUnitInvulnerable(udg_Statue, false)
    call DisableTrigger(gg_trg_Screaming)
    call EnableTrigger(gg_trg_Statue_Time_Limit)
endfunction

