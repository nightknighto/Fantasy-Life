    set udg_BonusStats[GetConvertedPlayerId(udg_Temp_Player)]=( udg_BonusStats[GetConvertedPlayerId(udg_Temp_Player)] + 2 )
    if ( Trig_Wine_Cask_Done_Func015C() ) then
        call ModifyHeroStat(bj_HEROSTAT_AGI, udg_Hero[GetConvertedPlayerId(udg_Temp_Player)], bj_MODIFYMETHOD_ADD, 1)
