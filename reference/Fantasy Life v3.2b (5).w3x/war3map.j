    endif
    set udg_KiEnhance[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=false
    set udg_Defend[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=false
    call ConditionalTriggerExecute(LoadTriggerHandleBJ(0, StringHashBJ(GetAbilityName(udg_BaseClass[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))])), udg_SkillTable))
