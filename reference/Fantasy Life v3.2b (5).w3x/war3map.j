function Trig_Spell_Enhance_Func004A takes nothing returns nothing
    if ( Trig_Spell_Enhance_Func004Func001C() ) then
        call UnitRemoveAbilityBJ('A058', GetEnumUnit())
        call UnitAddAbilityBJ('A058', GetEnumUnit())
        if ( Trig_Spell_Enhance_Func004Func001Func007C() ) then
            set udg_Temp_int=GetHeroStatBJ(bj_HEROSTAT_STR, GetEnumUnit(), true)
        else
            set udg_Temp_int=GetHeroStatBJ(bj_HEROSTAT_AGI, GetEnumUnit(), true)
        endif
        call SetUnitAbilityLevelSwapped('A058', GetEnumUnit(), udg_Temp_int)
        call UnitRemoveAbilityBJ('A059', GetEnumUnit())
        call UnitRemoveAbilityBJ('A05A', GetEnumUnit())
        call UnitRemoveAbilityBJ('A05C', GetEnumUnit())
        set udg_Temp_int=GetRandomInt(1, 3)
        if ( (udg_Temp_int == 1) ) then // INLINED!!
            call UnitAddAbilityBJ('A059', GetEnumUnit())
        else
            if ( (udg_Temp_int == 2) ) then // INLINED!!
                call UnitAddAbilityBJ('A05A', GetEnumUnit())
            else
                call UnitAddAbilityBJ('A05C', GetEnumUnit())
            endif
        endif
    else
        call UnitRemoveAbilityBJ('A058', GetEnumUnit())
        call UnitRemoveAbilityBJ('A059', GetEnumUnit())
        call UnitRemoveAbilityBJ('A05A', GetEnumUnit())
        call UnitRemoveAbilityBJ('A05C', GetEnumUnit())
