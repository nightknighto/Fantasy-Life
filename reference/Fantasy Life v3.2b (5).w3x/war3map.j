endfunction

function Trig_Banshee_Func040A takes nothing returns nothing
    set udg_POINT[100]=GetUnitLoc(GroupPickRandomUnit(GetUnitsOfPlayerMatching(GetEnumPlayer(), Condition(function Trig_Banshee_Func040Func001002001001002))))
    call ResetToGameCameraForPlayer(GetEnumPlayer(), 0)
    call PanCameraToTimedLocForPlayer(GetEnumPlayer(), udg_POINT[100], 0)
    call RemoveLocation(udg_POINT[100])
endfunction

function Trig_Banshee_Actions takes nothing returns nothing
    call RemoveUnit(udg_BossUnit)
    call PauseUnitBJ(false, gg_unit_H03O_0422)
    call PauseUnitBJ(false, gg_unit_H03L_0427)
    call PauseUnitBJ(false, gg_unit_H03N_0521)
    call PauseUnitBJ(false, gg_unit_H03M_0520)
    call PauseUnitBJ(false, gg_unit_H03P_0524)
    set udg_EventRunning=true
    call SetForceAllianceStateBJ(GetPlayersAll(), bj_FORCE_PLAYER[11], bj_ALLIANCE_UNALLIED)
    call SetForceAllianceStateBJ(bj_FORCE_PLAYER[11], GetPlayersAll(), bj_ALLIANCE_UNALLIED)
    call SetPlayerAllianceStateBJ(Player(PLAYER_NEUTRAL_PASSIVE), Player(11), bj_ALLIANCE_ALLIED)
    call PauseAllUnitsBJ(true)
    set udg_Cinematic=true
    call CinematicModeBJ(true, GetPlayersAll())
    call SetUserControlForceOff(GetPlayersAll())
    call CinematicFilterGenericBJ(12.00, BLEND_MODE_NONE, "ReplaceableTextures\\CameraMasks\\DreamFilter_Mask.blp", 0.01, 0.00, 0.00, 0.00, 0.01, 0.01, 0.01, 0.00)
    call TransmissionFromUnitWithNameBJ(GetPlayersAll(), udg_BossUnit, "TRIGSTR_2736", null, "TRIGSTR_2739", bj_TIMETYPE_ADD, 1.00, true)
    call TransmissionFromUnitWithNameBJ(GetPlayersAll(), udg_BossUnit, "TRIGSTR_3175", null, "TRIGSTR_3176", bj_TIMETYPE_ADD, 1.00, true)
    call DisplayCineFilterBJ(false)
    set udg_POINT[100]=GetRectCenter(gg_rct_Banshee_Start)
    call CreateNUnitsAtLoc(1, 'h04Y', Player(11), udg_POINT[100], bj_UNIT_FACING)
    call RemoveLocation(udg_POINT[100])
    set udg_BossUnit=GetLastCreatedUnit()
    call TriggerRegisterUnitEvent(gg_trg_Banshee_Attacked, GetLastCreatedUnit(), EVENT_UNIT_DAMAGED)
    call ForForce(GetPlayersAll(), function Trig_Banshee_Func025A)
    call TransmissionFromUnitWithNameBJ(GetPlayersAll(), udg_BossUnit2, "TRIGSTR_3177", null, "TRIGSTR_3178", bj_TIMETYPE_SUB, 1.00, true)
    set udg_POINT[100]=GetRectCenter(gg_rct_Banshee_Spawn)
    call IssuePointOrderLocBJ(udg_BossUnit, "move", udg_POINT[100])
    call RemoveLocation(udg_POINT[100])
    call TriggerSleepAction(8.00)
    call PlaySoundBJ(gg_snd_BansheeDeath)
    call SetUnitAnimation(udg_BossUnit, "spell")
    call TriggerSleepAction(3.00)
    call ForForce(GetPlayersAll(), function Trig_Banshee_Func034A)
    call TriggerSleepAction(3.00)
    call ForForce(GetPlayersAll(), function Trig_Banshee_Func036A)
    call TriggerSleepAction(2.00)
    call TransmissionFromUnitWithNameBJ(GetPlayersAll(), gg_unit_n01L_0072, "TRIGSTR_3185", null, "TRIGSTR_3186", bj_TIMETYPE_ADD, 0.00, true)
    call TransmissionFromUnitWithNameBJ(GetPlayersAll(), gg_unit_n00N_0278, "TRIGSTR_3187", null, "TRIGSTR_3188", bj_TIMETYPE_SUB, 1.00, true)
    call ForForce(GetPlayersAll(), function Trig_Banshee_Func040A)
    call CinematicModeBJ(false, GetPlayersAll())
    call SetUserControlForceOn(GetPlayersAll())
    set udg_Cinematic=false
    call PauseAllUnitsBJ(false)
    call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_2665")
    call GroupAddUnitSimple(udg_BossUnit, udg_Bosses)
    call SaveStringBJ("Uggg.....the screaming reminds me of the time my sister got on the mic on karaoke night!", StringHashBJ(UnitId2StringBJ('n00N')), 0, udg_TalkingTable)
    call EnableTrigger(gg_trg_Screaming)
    call EnableTrigger(gg_trg_Banshee_Attacked)
    call EnableTrigger(gg_trg_Statue_Dies)
    call EnableTrigger(gg_trg_Statue_Damaged)
    call EnableTrigger(gg_trg_Stay_in_the_Hall)
    call EnableTrigger(gg_trg_Kain)
    call EnableTrigger(gg_trg_Link)
    call EnableTrigger(gg_trg_Ryu)
    call EnableTrigger(gg_trg_Ryu_Hayabusa)
    call EnableTrigger(gg_trg_Vivi)
    call EnableTrigger(gg_trg_Banshee_Victory)
    call EnableTrigger(gg_trg_Banshee_Defeat)
    call SetUnitInvulnerable(gg_unit_h03K_0322, false)
endfunction

