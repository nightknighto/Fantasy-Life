    call DisableTrigger(gg_trg_Throne_Captured)
    call TriggerExecute(gg_trg_Open_All_Doors)
    set udg_Temp_Group3=GetUnitsOfPlayerAndTypeId(GetOwningPlayer(gg_unit_h050_0042), 'h023')
    call ForGroupBJ(udg_Temp_Group3, function Trig_Siege_Begins_Func005A)
    call DestroyGroup(udg_Temp_Group3)
    set udg_UNITGROUP[1]=GetUnitsInRectOfPlayer(gg_rct_Western_Highway, Player(11))
    call ForGroupBJ(udg_UNITGROUP[1], function Trig_Siege_Begins_Func008A)
    call DestroyGroup(udg_UNITGROUP[1])
    set udg_BanditTemp=( 5.00 * ( ( ( I2R(udg_Happiness) / 1000.00 ) * ( I2R(udg_Happiness) / 1000.00 ) ) - ( ( I2R(udg_Happiness) / 1000.00 ) - 2.00 ) ) )
    set udg_Temp_Point_3=GetRectCenter(gg_rct_Gate_Attack)
    call SetUnitPositionLoc(udg_BossUnit2, udg_Temp_Point_3)
    call RemoveLocation(udg_Temp_Point_3)
    set udg_Temp_Point_3=GetRectCenter(gg_rct_Leader_Position)
    call SetUnitPositionLoc(udg_BossUnit, udg_Temp_Point_3)
    call RemoveLocation(udg_Temp_Point_3)
    call SetUnitInvulnerable(gg_unit_h03Q_0144, false)
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=R2I(( udg_BanditTemp * 0.25 ))
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        set udg_Temp_Point_3=GetRectCenter(gg_rct_Left_Wall)
        call CreateNUnitsAtLoc(2, 'h02L', Player(11), udg_Temp_Point_3, bj_UNIT_FACING)
        call RemoveLocation(udg_Temp_Point_3)
        set udg_Temp_Point_3=GetRectCenter(gg_rct_Right_Wall)
        call CreateNUnitsAtLoc(2, 'h02L', Player(11), udg_Temp_Point_3, bj_UNIT_FACING)
        call RemoveLocation(udg_Temp_Point_3)
        set udg_Temp_Point_3=GetRectCenter(gg_rct_Gate_Attack)
        call CreateNUnitsAtLoc(2, 'h038', Player(11), udg_Temp_Point_3, bj_UNIT_FACING)
        call RemoveLocation(udg_Temp_Point_3)
        set udg_Temp_Point_3=GetRectCenter(gg_rct_Leader_Position)
        call CreateNUnitsAtLoc(1, 'h038', Player(11), udg_Temp_Point_3, bj_UNIT_FACING)
        call RemoveLocation(udg_Temp_Point_3)
        set udg_Temp_Point_3=GetRectCenter(gg_rct_Crumbling_Wall_Attack)
        call CreateNUnitsAtLoc(1, 'h038', Player(11), udg_Temp_Point_3, bj_UNIT_FACING)
        call RemoveLocation(udg_Temp_Point_3)
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    set udg_UNITGROUP[1]=GetUnitsInRectOfPlayer(gg_rct_Gate_Attack, Player(11))
    set udg_UNITGROUP[2]=GetUnitsInRectOfPlayer(gg_rct_Left_Wall, Player(11))
    set udg_Temp_Group3=GetUnitsInRectOfPlayer(gg_rct_Right_Wall, Player(11))
    call GroupAddGroup(udg_Temp_Group3, udg_UNITGROUP[2])
    call DestroyGroup(udg_Temp_Group3)
    set udg_UNITGROUP[3]=GetUnitsInRectOfPlayer(gg_rct_Crumbling_Wall_Attack, Player(11))
    set udg_UNITGROUP[6]=GetUnitsInRectOfPlayer(gg_rct_Leader_Position, Player(11))
    call SetPlayerAllianceStateBJ(Player(11), Player(PLAYER_NEUTRAL_PASSIVE), bj_ALLIANCE_UNALLIED)
    call SaveStringBJ("Griffith: The time has come, take up your arms, when the men charge they will need your assistance.", StringHashBJ(UnitId2StringBJ('H03I')), 0, udg_TalkingTable)
    call EnableTrigger(gg_trg_Attacking_the_Front_Gate)
    call ForGroupBJ(udg_UNITGROUP[1], function Trig_Siege_Begins_Func029A)
    call ForGroupBJ(udg_UNITGROUP[2], function Trig_Siege_Begins_Func030A)
    call ForGroupBJ(udg_UNITGROUP[3], function Trig_Siege_Begins_Func031A)
    set udg_Temp_Group3=GetUnitsInRectOfPlayer(gg_rct_Leader_Position, Player(11))
    call ForGroupBJ(udg_Temp_Group3, function Trig_Siege_Begins_Func033A)
    call DestroyGroup(udg_Temp_Group3)
    call ForceAddPlayerSimple(GetOwningPlayer(gg_unit_h050_0042), udg_UnknownsEnemies)
    call SetPlayerAllianceStateBJ(Player(11), GetOwningPlayer(gg_unit_h050_0042), bj_ALLIANCE_UNALLIED)
    call SetPlayerAllianceStateBJ(GetOwningPlayer(gg_unit_h050_0042), Player(11), bj_ALLIANCE_UNALLIED)
    set udg_TalkedtoBoss[GetConvertedPlayerId(GetOwningPlayer(gg_unit_h050_0042))]=true
endfunction

