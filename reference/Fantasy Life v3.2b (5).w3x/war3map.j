        return false
    endif
    return true
endfunction

function Trig_Dragon_Combo_Actions takes nothing returns nothing
    set udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())]=GetSpellTargetUnit()
    set udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())]=GetSpellAbilityUnit()
    call PauseUnitBJ(true, udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())])
    call PauseUnitBJ(true, udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())])
    call SetUnitInvulnerable(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], true)
    set udg_Loop_Temp=1
    loop
        exitwhen udg_Loop_Temp > ( ( 4 + GetHeroStatBJ(bj_HEROSTAT_AGI, udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], true) ) / 3 )
        call SetUnitPositionLocFacingLocBJ(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], PolarProjectionBJ(GetUnitLoc(udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())]), 100.00, GetRandomDirectionDeg()), GetUnitLoc(udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())]))
        call SetUnitAnimation(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], "attack")
        if ( Trig_Dragon_Combo_Func006Func003C() ) then
            call UnitDamageTargetBJ(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())], ( I2R(GetHeroStatBJ(bj_HEROSTAT_STR, udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], true)) * 2.00 ), ATTACK_TYPE_NORMAL, DAMAGE_TYPE_NORMAL)
        else
            call PauseUnitBJ(false, udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())])
            call SetUnitInvulnerable(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], false)
            return
        endif
        call TriggerSleepAction(0.20)
        set udg_Loop_Temp=udg_Loop_Temp + 1
    endloop
    call TriggerSleepAction(0.20)
    call SetUnitPositionLocFacingLocBJ(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], PolarProjectionBJ(GetUnitLoc(udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())]), 100.00, GetRandomDirectionDeg()), GetUnitLoc(udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())]))
    call SetUnitAnimation(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], "attack")
    call TriggerSleepAction(0.20)
    call UnitDamageTargetBJ(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())], ( I2R(GetHeroStatBJ(bj_HEROSTAT_STR, udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], true)) * 3.00 ), ATTACK_TYPE_NORMAL, DAMAGE_TYPE_NORMAL)
    call UnitAddAbilityBJ('Amrf', udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())])
    call UnitRemoveAbilityBJ('Amrf', udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())])
    call SetUnitFlyHeightBJ(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], 100.00, 3333.00)
    call UnitAddAbilityBJ('Amrf', udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())])
    call UnitRemoveAbilityBJ('Amrf', udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())])
    call SetUnitFlyHeightBJ(udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())], 100.00, 3333.00)
    call SetUnitAnimation(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], "attack")
    call TriggerSleepAction(0.50)
    call UnitDamageTargetBJ(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())], ( I2R(GetHeroStatBJ(bj_HEROSTAT_STR, udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], true)) * 2.00 ), ATTACK_TYPE_NORMAL, DAMAGE_TYPE_NORMAL)
    call SetUnitFlyHeightBJ(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], 200.00, 3333.00)
    call SetUnitFlyHeightBJ(udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())], 200.00, 3333.00)
    call SetUnitAnimation(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], "attack")
    call AddSpecialEffectTargetUnitBJ("chest", udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], "units\\creeps\\RedDragon\\RedDragon.mdl")
    set udg_DragonComboEffect2=GetLastCreatedEffectBJ()
    call TriggerSleepAction(0.50)
    call UnitDamageTargetBJ(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())], ( I2R(GetHeroStatBJ(bj_HEROSTAT_STR, udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], true)) * 2.00 ), ATTACK_TYPE_NORMAL, DAMAGE_TYPE_NORMAL)
    call DestroyEffectBJ(udg_DragonComboEffect2)
    call SetUnitFlyHeightBJ(udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())], 0.00, 3333.00)
    call TriggerSleepAction(0.10)
    call AddSpecialEffectLocBJ(GetUnitLoc(udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())]), "Abilities\\Spells\\Orc\\WarStomp\\WarStompCaster.mdl")
    set udg_DragonComboEffect=GetLastCreatedEffectBJ()
    call TriggerSleepAction(0.30)
    call SetUnitFlyHeightBJ(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], 0.00, 3333.00)
    call UnitDamageTargetBJ(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())], ( I2R(GetHeroStatBJ(bj_HEROSTAT_STR, udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], true)) * 2.00 ), ATTACK_TYPE_NORMAL, DAMAGE_TYPE_NORMAL)
    call DestroyEffectBJ(udg_DragonComboEffect)
    call PauseUnitBJ(false, udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())])
    call PauseUnitBJ(false, udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())])
    call SetUnitInvulnerable(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())], false)
    call SetUnitInvulnerable(udg_DragonComboTarget[GetConvertedPlayerId(GetTriggerPlayer())], false)
    call ResetUnitAnimation(udg_DragonComboCaster[GetConvertedPlayerId(GetTriggerPlayer())])
endfunction

