//===========================================================================
// Trigger: Lancet
//===========================================================================
function Trig_Lancet_Func003C takes nothing returns boolean
    if ( not ( GetUnitStateSwap(UNIT_STATE_MANA, GetSpellTargetUnit()) >= ( I2R(GetHeroStatBJ(bj_HEROSTAT_AGI, GetTriggerUnit(), true)) * 0.50 ) ) ) then
        return false
    endif
    return true
endfunction

function Trig_Lancet_Actions takes nothing returns nothing
    call UnitDamageTargetBJ(GetTriggerUnit(), GetSpellTargetUnit(), ( I2R(GetHeroStatBJ(bj_HEROSTAT_AGI, GetTriggerUnit(), true)) * 2.00 ), ATTACK_TYPE_NORMAL, DAMAGE_TYPE_NORMAL)
    call SetUnitLifeBJ(GetTriggerUnit(), ( GetUnitStateSwap(UNIT_STATE_LIFE, GetTriggerUnit()) + ( I2R(GetHeroStatBJ(bj_HEROSTAT_AGI, GetTriggerUnit(), true)) + 0.00 ) ))
    if ( Trig_Lancet_Func003C() ) then
        call SetUnitManaBJ(GetTriggerUnit(), ( GetUnitStateSwap(UNIT_STATE_MANA, GetTriggerUnit()) + ( I2R(GetHeroStatBJ(bj_HEROSTAT_AGI, GetTriggerUnit(), true)) * 0.50 ) ))
        call SetUnitManaBJ(GetSpellTargetUnit(), ( GetUnitStateSwap(UNIT_STATE_MANA, GetSpellTargetUnit()) - ( I2R(GetHeroStatBJ(bj_HEROSTAT_AGI, GetTriggerUnit(), true)) * 1.00 ) ))
    else
        call SetUnitManaBJ(GetTriggerUnit(), ( GetUnitStateSwap(UNIT_STATE_MANA, GetTriggerUnit()) + GetUnitStateSwap(UNIT_STATE_MANA, GetSpellTargetUnit()) ))
        call SetUnitManaBJ(GetSpellTargetUnit(), 0.00)
    endif
endfunction

